SET 'auto.offset.reset'='earliest';

/* COUNT ACCESES */
CREATE TABLE IF NOT EXISTS ADW06_NACC
	WITH (KAFKA_TOPIC='adw.06.nacc', VALUE_FORMAT='AVRO', KEY_FORMAT='AVRO', PARTITIONS=2, REPLICAS=2)
	AS SELECT
		STRUCT(IP:=IP, UAID:=UAID) as KEY,
		COUNT(*) as N
	FROM ADW06_KEYACC
	GROUP BY STRUCT(IP:=IP, UAID:=UAID);
SET 'auto.offset.reset'='earliest';

/* COUNT ACCESES */
CREATE TABLE IF NOT EXISTS adw06_nacc
	WITH (KAFKA_TOPIC='adw.adw06.nacc', VALUE_FORMAT='AVRO', KEY_FORMAT='AVRO', PARTITIONS=2, REPLICAS=2)
	AS SELECT
		STRUCT(IP:=IP, UAID:=UAID) as KEY,
		COUNT(*) as N
	FROM adw06_keyacc 
	GROUP BY STRUCT(IP:=IP, UAID:=UAID);
SET 'auto.offset.reset'='earliest';

/* Q2 */
SELECT KM.KEY AS KEY,
	ARRAY[MAX(KM.UV)] AS UV,
	COLLECT_SET(KA.METHOD) AS MET,
	COLLECT_SET(KA.STATUS_CODE) AS STC,
	COUNT(KA.IP) AS N
FROM adw06_keymon KM
	INNER JOIN adw06_keyacc KA
	WITHIN 300 DAYS
	GRACE PERIOD 15 MINUTES
	ON KA.KEY = KM.KEY
WHERE KM.KEY->UAID= '6248c7a654ab4c1061918e0801cb074f5f01983d'
GROUP BY KM.KEY
EMIT CHANGES;